<!-- Make the fuel selector valve move based on the value of switch-position 
     The actual change in tank selection is done by pa24.nas which allows
     cycling the selected tank by keyboard "f".-->
 <animation>
  <type>rotate</type>
  <object-name>FuelValve</object-name>
  <factor>-72</factor>
   <property>controls[1]/fuel/switch-position</property>  
  <center>
   <x-m>2.666</x-m>
   <y-m>0.0</y-m>
   <z-m>-0.505</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>

 <animation>
  <type>material</type>
  <object-name>Beacon</object-name>
  <emission>
   <red>1.0</red>
   <green>0.0</green>
   <blue>0.0</blue>
   <factor-prop>/sim/model/lights/beacon/state</factor-prop>
  </emission>
 </animation>

 <animation>
