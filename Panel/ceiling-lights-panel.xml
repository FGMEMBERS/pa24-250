<?xml version="1.0"?>

<!DOCTYPE PropertyList SYSTEM "panel.dtd">

<PropertyList>

 <name>ceiling-Panel</name>
 <background>Aircraft/pa24-250/Panel/transparent-bg.rgb</background>
 <w>1024</w>
 <h>768</h>

<instruments>

<instrument>
  <name>panel hotspots</name>
  <x>512</x>
  <y>384</y>
  <w>1024</w>
  <h>768</h>
  <w-base>1024</w-base>
  <h-base>768</h-base>

 <actions> 

   <action>
     <name>ToggleMapLight</name>
     <button>0</button>
     <x>-64</x>
     <y>-128</y>
     <w>128</w>
     <h>192</h>
     <binding>
       <command>nasal</command>
       <script>pa24.map_light_switch</script>
     </binding>
   </action>
  
   <action>
     <name>ToggleCabinLight</name>
     <button>0</button>
     <x>64</x>
     <y>128</y>
     <w>128</w>
     <h>192</h>
     <binding>
       <command>nasal</command>
       <script>pa24.cabin_light_switch</script>
     </binding>
   </action>
     
  </actions>

 </instrument>

</instruments>

</PropertyList>
